{
  "presets": [
    "es2015",
    "stage-3",
    [
      "env",
      {
        "modules": false,
        "useBuiltIns": false
      }
    ]
  ],
  "plugins": [
    "syntax-dynamic-import"
  ],
  "env": {
    "test": {
      "presets": [
        "es2015",
        "stage-3"
      ],
      "plugins": [
        "transform-runtime"
      ]
    }
  },
  "ignore": [
    "dist/*.js"
  ]
}
